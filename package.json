{
  "name": "table-tennis-results-tracker",
  "version": "1.0.0",
  "author": {
    "name": "Matej Buljan",
    "email": "matejb6@icloud.com"
  },
  "description": "An Angular web app for tracking table tennis results",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "prettier:check": "npx prettier --check ./src",
    "prettier:write": "npx prettier --write ./src",
    "lint": "ng lint",
    "test": "ng test --browsers=Chrome",
    "test:ci": "ng test --browsers=ChromeHeadless --reporters=spec --no-watch",
    "test:coverage": "ng test --browsers=ChromeHeadless --reporters=spec --no-watch --code-coverage",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.4",
    "@angular/cdk": "^14.2.3",
    "@angular/common": "^14.2.4",
    "@angular/compiler": "^14.2.4",
    "@angular/core": "^14.2.4",
    "@angular/forms": "^14.2.4",
    "@angular/material": "^14.2.3",
    "@angular/platform-browser": "^14.2.4",
    "@angular/platform-browser-dynamic": "^14.2.4",
    "@angular/router": "^14.2.4",
    "rxjs": "^7.5.7",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.4",
    "@angular-eslint/builder": "^14.1.2",
    "@angular-eslint/eslint-plugin": "^14.1.2",
    "@angular-eslint/eslint-plugin-template": "^14.1.2",
    "@angular-eslint/schematics": "^14.1.2",
    "@angular-eslint/template-parser": "^14.1.2",
    "@angular/cli": "^14.2.4",
    "@angular/compiler-cli": "^14.2.4",
    "@types/jasmine": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jasmine-core": "^4.4.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "karma-spec-reporter": "0.0.34",
    "prettier": "^2.7.1",
    "typescript": "~4.7.2"
  },
  "keywords": [
