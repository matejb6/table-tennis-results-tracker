{
  "name": "table-tennis-results-tracker",
  "version": "1.1.1",
  "description": "An Angular web app for tracking table tennis results",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "prettier:check": "prettier --check ./src",
    "prettier:write": "prettier --write ./src",
    "lint": "ng lint",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "test:coverage": "ng test --no-watch --no-progress --browsers=ChromeHeadless --code-coverage",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.10",
    "@angular/compiler-cli": "^15.2.0",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^8.0.3",
    "jasmine-core": "^4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^3.1.1",
    "typescript": "~4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matejb6/table-tennis-results-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/matejb6/table-tennis-results-tracker/issues"
  },
  "keywords": [
    "matejb6",
    "Angular",
    "Forms",
    "Results",
    "Assignment"
  ],
  "author": {
    "name": "Matej Buljan",
    "email": "matejb6@icloud.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 14.20.0"
  }
}
