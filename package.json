{
  "name": "table-tennis-results-tracker",
  "version": "1.3.0",
  "description": "An Angular web app for tracking table tennis results",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "prettier:check": "prettier --check ./src",
    "prettier:write": "prettier --write ./src",
    "lint": "ng lint",
    "test": "ng test",
    "test:ci": "ng test --no-watch --no-progress --browsers=ChromeHeadless",
    "test:coverage": "ng test --no-watch --no-progress --browsers=ChromeHeadless --code-coverage",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.11",
    "@angular-eslint/builder": "17.5.3",
    "@angular-eslint/eslint-plugin": "17.5.3",
    "@angular-eslint/eslint-plugin-template": "17.5.3",
    "@angular-eslint/schematics": "17.5.3",
    "@angular-eslint/template-parser": "17.5.3",
    "@angular/cli": "^17.3.11",
    "@angular/compiler-cli": "^17.3.0",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.3.3",
    "typescript": "~5.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matejb6/table-tennis-results-tracker.git"
  },
  "bugs": {
    "url": "https://github.com/matejb6/table-tennis-results-tracker/issues"
  },
  "keywords": [
    "matejb6",
    "Angular",
    "Forms",
    "Results",
    "Assignment"
  ],
  "author": {
    "name": "Matej Buljan",
    "email": "matejb6@icloud.com"
  },
  "license": "MIT",
  "engines": {
    "node": "^18.13.0 || ^20.9.0"
  }
